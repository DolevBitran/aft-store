{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "components/*": [
        "components/*"
      ],
      "views/*": [
        "views/*"
      ],
      "store": [
        "store/"
      ],
      "store/*": [
        "store/*"
      ],
      "utils": [
        "utils/index.ts"
      ],
      "utils/*": [
        "utils/*"
      ],
      "service/*": [
        "service/*"
      ],
      "mock/*": [
        "mock/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "constants/*": [
        "constants/*"
      ],
      "hooks/*": [
        "hooks/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
