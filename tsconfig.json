{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    // "allowSyntheticDefaultImports": true,
    // "jsx": "react-native",
    // "lib": [
    //   "dom",
    //   "esnext"
    // ],
    // "experimentalDecorators": true,
    // "emitDecoratorMetadata": true,
    // "moduleResolution": "node",
    // "module": "commonjs",
    // "noEmit": true,
    // "target": "es6",
    // "skipLibCheck": true,
    // "resolveJsonModule": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "src/components/*"
      ],
      "views/*": [
        "src/views/*"
      ],
      "store": [
        "src/store/"
      ],
      "store/*": [
        "src/store/*"
      ],
      "utils": [
        "src/utils/index.ts"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "service/*": [
        "src/service/*"
      ],
      "mock/*": [
        "src/mock/*"
      ],
      "assets/*": [
        "assets/*"
      ]
    },
  },
  "exclude": [
    "node_modules"
  ]
}